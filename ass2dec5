n=int(input("enter how many numbers you want to enter: "))
num=int(input("enter a number: "))
largest=num
smallest=num
i=1
while i<n:
    num=int(input("enter a number: "))
    if num>largest:
        largest=num
    if num<smallest:
        smallest=num
    i=i+1    
print(largest)
print(smallest)
difference=largest-smallest
print(difference)
